<!--big_pic-->
<div class="big_pic">
  
  <!--big_pic_main-->
  <div class="big_pic_main">
    <div class="big_pic_img">
      <a class="big_show" href="javascript:;">
        <span class="big-loading"></span>
        <img class="show_pic" src="">
      </a>
      <a class="big_pic_left" href="javascript:;"><i></i></a>
      <a class="big_pic_right" href="javascript:;"><i></i></a>
    </div>
    <div class="big_pic_info">
      <ul class="more_info_main">
        <li class="big_pic_func" style="display:none;">
          <a href="javascript:;" class="big_pic_coll"><i></i><span>收藏</span></a>
          <a href="javascript:;" class="big_pic_like"><i></i><span>赞</span></a>
        </li>
        <li class="big_pic_msg">
          <a href="javascript:;">
            <i>from.</i> 
            <h4 class="more_info_from"></h4>
          </a>
        </li>
        <li class="big_pic_msg">
          <a href="javascript:;">
            <i>by.</i> 
            <h4 class="author"></h4>
          </a>
        </li>
        <!--分享-->
        <li class="big_pic_share">
          <div class="spec_share_main" style="display:none;">
              <span>分享：</span>
              <a class="sina" href="javascript:;"><i></i></a>
              <a class="weixin" href="javascript:;"><i></i></a>
              <a class="friend" href="javascript:;"><i></i></a>
              <a class="qq" href="javascript:;"><i></i></a>
              <a class="qzone" href="javascript:;"><i></i></a>
          </div>
        </li>
      </ul>
    </div>
  </div>
