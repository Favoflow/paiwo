var status_login = 1;
var status_register = 0;
var status_photographer = 0;
var status_findpwd = 0;



function setStatus(str){
	 	status_login        = 0;
		status_register     = 0;
		status_photographer = 0;
		status_findpwd      = 0;
		if(str == 'login'){
			status_login =1
		}else if(str == 'regis'){
			status_register = 1
		}else if(str == 'photographer'){
			status_photographer = 1
		}else{
			status_findpwd = 1;
		}

} 