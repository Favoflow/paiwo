/*搜索*/
.search-sim {
  position: fixed; 
  top: -80px;
  left: 0;
  z-index: 97;
  width: 100%;
  height: 80px;
  line-height: 80px;
  background: rgba(33,33,33,.9);
  opacity: 0;
  -webkit-transform:translate3d(0,0,0);
  -moz-transform:translate3d(0,0,0);
  -ms-transform:translate3d(0,0,0);
  transform:translate3d(0,0,0);
  opacity: 0;
	
  -webkit-transition:all 0.4s;
  -moz-transition:all 0.4s;
  -ms-transition:all 0.4s;
  transition:all 0.4s;
}

.search-sim.active{
	-webkit-transform:translate3d(0,120px,0);
	-moz-transform:translate3d(0,120px,0);
	-ms-transform:translate3d(0,120px,0);
	transform:translate3d(0,120px,0);
	opacity: 1;
}

.search-sim.moveIn{
	-webkit-transform:translate(240px,120px);
	-moz-transform:translate(240px,120px);
	-ms-transform:translate(240px,120px);
	transform:translate(240px,120px);
	-webkit-transition:-webkit-transform 0.5s;
	-moz-transition:-moz-transform 0.5s;
	-ms-transition:-ms-transform 0.5s;
	transition:transform 0.5s;
}

.search-sim.moveOut{
	-webkit-transform:translateX(0px,120px);
	-moz-transform:translateX(0px,120px);
	-ms-transform:translateX(0px,120px);
	transform:translateX(0px,120px);
	-webkit-transition:-webkit-transform 0.5s;
	-moz-transition:-moz-transform 0.5s;
	-ms-transition:-ms-transform 0.5s;
	transition:transform 0.5s;
}

.search-sim .search-box { margin-top: 0;}
.search-sim .search-box input { background: rgba(65,65,65,.8);}
.search-sim .search-box a { top: 18px; right: -11px;}
.search-sim .search-word { top: 17px;}
