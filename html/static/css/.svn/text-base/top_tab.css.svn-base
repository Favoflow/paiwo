/*tab-contet*/
.top-tab {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 98;
  width: 100%;
  height: 40px;
  display: box;
  display: -webkit-box;
  display: -moz-box;
  box-orient: horizontal;
  -moz-box-orient: horizontal;
  -webkit-box-orient: horizontal;
	-webkit-transform:translateX(0);
	-moz-transform:translateX(0);
	-ms-transform:translateX(0);
	transform:translateX(0);
	-webkit-transition:-webkit-transform 0.5s;
	-moz-transition:-moz-transform 0.5s;
	-ms-transition:-ms-transform 0.5s;
	transition:transform 0.5s;
}
.top-tab.active {
	-webkit-transform:translateX(240px);
	-moz-transform:translateX(240px);
	-ms-transform:translateX(240px);
	transform:translateX(240px);
}
.top-tab-logo {
  left: 60px;
  top: 40px;
  text-indent: -999em;
  background: url(/static/images/spirit.svg) no-repeat -200px -400px; 
}
.top-tab-logo a {
  display: block;
  width: 56px;
  height: 24px;
}
.top-tab-con {
  box-flex: 1;
  -moz-box-flex: 1;
  -webkit-box-flex: 1;
  height: 40px;
  font-size: 0;
  position: relative;
  background-color: #fff;
  box-shadow: 0 0 1px 1px rgba(150, 150, 150,.4);
  opacity: 0.97;
}

#tab-bephotog {
  display: none;
  position: absolute;
  right: 145px;
  width: 77px;
  height: 24px;
  border: 1px solid #eee;
  border-radius: 2px;
  margin: 7px 8px 0 0;
  text-align: center;
  line-height: 24px;
  font-size: 12px;
  color: #a8a8a8;
}
.tab-icon {
  position: relative;
  float: right;
  width: 40px;
  height: 40px;
  border-left: 1px solid #eee;
  color: #a8a8a8;
  display: none;
}
.tab-icon a {
  display: block;
  width:  40px;
  height: 40px;
  opacity: 0.8;
}
.tab-icon i {
  position: absolute;
  top: 10px;
  right: 10px;
  width: 6px;
  height: 6px;
  border-radius: 3px;
  background: #ff3c3c;
  display: none;
}
.tab-icon:hover { background: #f3f2f2; }
.tab-icon a:hover { 
  opacity: 1;
  filter: alpha(opacity=100); 
}
.tab-icon_file { background: url(/static/images/spirit.svg) no-repeat 12px -89px; }
.tab-icon_mybrowse { background: url(/static/images/spirit.svg) no-repeat -88px -89px; }
.tab-icon_mystore { background: url(/static/images/spirit.svg) no-repeat -188px -88px; }
.tab-icon_mylike { background: url(/static/images/spirit.svg) no-repeat -288px -86px; }
.tab-icon_myme { background: url(/static/images/spirit.svg) no-repeat -38px -186px; }
.tab-icon_myset { background: url(/static/images/spirit.svg) no-repeat 12px -187px; }
#tab-bephotog:hover { background-color: #f3f2f2;}

.top-tab .tab_select_box{
	width: 123px;
	height: 0;
	position: absolute;
	top: 40px;
	right: 0;
	opacity: 0;
	overflow: hidden;
}
.top-tab .tab_select {
  position: absolute;
  top: 0;
  right: 0;
  width: 121px;
  height: 161px;
  background: #fff;
  border: 1px solid #eee;
  box-shadow: 0 0 5px 0 rgba(150, 150, 150, .3);
}
.tab_select a {
  display: block;
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  text-align: center;
  color: #7a7a7a;
  cursor: pointer;
}
.tab_select a:hover {
  background: #f3f2f2;
  color: #414141;
  outline: 1px solid #eee; 
}
.tab_select .select_login_out:hover { color: #dd2338;}
.tab_searchbox {
  box-flex: 1;
  -moz-box-flex: 1;
  -webkit-box-flex: 1;
  height: 29px;
  position: relative;
  background-color: #fff;
  box-shadow: 0 0 1px 1px rgba(150, 150, 150,.4);
  opacity: 0.97;
}
.tab_search {
  width: 635px;
  height: 23px;
  border: 1px solid #dfdede;
  box-shadow: 0 0 3px 0 rgba(150, 150, 150, .3) inset;
  background: #f7f7f6;
  overflow: hidden;
  border-radius: 2px;
  position: absolute;
  left: 0;
  top: 5px;
  padding: 2px 0 2px 3px;
}
.search_button {
  position: absolute;
  top: 6px;
  right: 8px;
  width: 16px;
  height: 16px;
  background: url(/static/images/spirit.svg) no-repeat -200px -800px;  
}
.tab_search_ul { 
  width: 10000px;
  height: 22px;
}
.tab_search_ul li {
  border-radius: 2px;
  height: 24px;
  float: left;
  margin-right: 5px;
}
.tab_search_ul li div {
  box-shadow: inset 0 0 2px rgba(0, 0, 0, .2);
  color: #fff;
  cursor: pointer;
  float: left;
  font-size: 12px;
  font-weight: 300;
  height: 22px;
  border-radius: 2px;
  background: #ff6674;
  padding-left: 7px;
}
.tab_search_ul li div span {
  float: left;
  height: 22px;
  line-height: 22px;
}
.search_ul_remove {
  border-left: 1px dotted rgba(255, 255, 255, .18);
  border-color: #fff;
  float: left;
  margin-left: 7px;
}
.search_ul_remove i {
  display: block;
  width: 8px;
  height: 8px;
  margin: 7px 5px;
  background: url(/static/images/spirit.svg) no-repeat -150px -2100px;
}
.tab_search_text {
  padding: 2px 0 2px 4px;
  width: 100%;
  border-radius: 2px;
  height: 24px;
  float: left;
  margin-right: 5px;
}
















