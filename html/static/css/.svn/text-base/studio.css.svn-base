#main_box { padding-top: 40px;}
.photo_fixbox { cursor: auto;}
.select-arrow {
  -webkit-transform-origin:4px 4px;
  -ms-transform-origin:4px 4px;
  transform-origin:4px 4px;
}
.stuido_mian_bigbox {
  width: 100%;
  padding-top: 60px;
}
.stuido_mian {
  width: 1190px;
  background: #fff;
  margin: 0 auto;
}

/*相册信息*/
.title_photo_ava {
    width: 60px;
    height: 60px;
    position: relative;
    display: block;
    margin: 12px auto;
}
.title_photo_ava img {
  width: 60px;
  height: 60px;
  border-radius: 50%;
}
.title_photo_ava i {
  position: absolute;
  right: -11px;
  top: -2px;
  width: 24px;
  height: 24px;
  border-radius: 50%;
  background: url(/static/images/spirit2.svg) no-repeat 2px -348px #fff;
}

.photog-male { background: url(/static/images/spirit.svg) no-repeat scroll -100px -800px; }
.photog-female { background: url(/static/images/spirit.svg) no-repeat scroll 0 -800px;}

.photobox_title_intro {
  width: 355px;
  color: #b6b3ad;
  position: relative;
  margin: 19px auto 0 auto;
  text-align: center; 
}
.photobox_title_intro span {
  display: block;
  width: 100%;
  line-height: 20px;
  overflow: hidden;
  text-align: center;
  font-size: 12px;
}
.photobox_title_intro a {
  position: absolute;
  height: 20px;
  line-height: 20px;
  color: #dd2338;
  left: 165px;
  bottom: -25px;
  font-size: 12px;
}
.content_photobox {
  margin-top: 37px;
  min-height: 600px;
}
.content_photobox_title {
  padding-top: 32px;
  width: 1190px;
  margin: 0 auto;
  position: relative;
  color: #868686;
}
.title_photo_delete,
.title_photo_edit {
  position: absolute;
  width: 50px;
  font-size: 12px;
  height: 14px;
  color: #b6b3ad;
  top: 11px;
  text-align: left;
  cursor: pointer;
}
.title_photo_edit { right: 11px;}
.title_photo_delete {
  right: 100px;
  display: none;
}
.title_photo_delete i,
.title_photo_edit i { 
  position: absolute;
  width: 12px;
  height: 13px;
  left: -20px;
}
.title_photo_edit i { background: url(/static/images/spirit2.svg) no-repeat -50px -100px; margin-top: 2px;}
.title_photo_delete i { background: url(/static/images/spirit2.svg) no-repeat -50px -150px; margin-top: 1px;}
.content_photobox .title_photo_edit-ihover { background: url(/static/images/spirit2.svg) no-repeat -100px -100px;}
.content_photobox .title_photo_delete-ihover { background: url(/static/images/spirit2.svg) no-repeat -100px -150px;}
.title_photo_edit:hover { color: #686868;}
.title_photo_delete:hover { color: #dd2222;}
.content_photobox_title h2 {
  font-size: 48px;
  color: #414141;
  height: 86px;
  line-height: 86px;
  text-align: center;
  font-weight: normal;
}
.content_photobox_title h3 {
  height: 13px;
  font-size: 12px;
  line-height: 13px;
  color: #b6b3ad;
  margin-top: 8px;
  text-align: center;
  font-weight: normal;
}
.photog_introduction .title_photo_edit {
  position: relative;
  display: block;
  width: 65px;
  height: 30px;
  padding-left: 35px;
  border: 1px solid #b6b3aa;
  border-radius: 15px;
  color: #686868;
  line-height: 30px;
  margin: 0 auto;
}
.photog_introduction .title_photo_edit i {
  position: absolute;
  left: 15px;
  top: 7px;
  background: url(/static/images/spirit2.svg) no-repeat -100px -100px;
}
.photog_introduction .photog_intro_text { width: 280px;}
.content_photobox_title h3 span{ padding: 0 2px;}
.content_photobox .comment_tab_share {
 position: absolute;
 bottom: -48px;
 right: 0; 
}
.content_photobox .comment_tab_share:after { 
  display: block;
  clear: both;
  content: "";
}
.comment_tab_share span {
  float: left;
  color: #8a8880;
  line-height: 32px;
  font-size: 12px;
}
.album_comment_tab_copyright {
  height: 20px;
  margin-top: 10px;
  max-width: 260px;
  position: absolute;
  bottom: -48px;
  left: 0;
}
.album_comment_tab_copyright i {
  width: 16px;
  height: 16px;
  background-image: url("/static/images/upload_pg.png");
  background-repeat: no-repeat;
  float: left;
  height: 20px;
  margin-right: 5px;
  width: 16px;
  cursor: auto;
}
.album_comment_tab_copyright a{ color: #868686;}
.album_comment_tab_copyright span { cursor: pointer;}
.album_comment_tab_copyright .type1 { background-position: -102px 0px;}
.album_comment_tab_copyright .type2 { background-position: -62px 0px;}
.album_comment_tab_copyright .type3 { background-position: -3px 0px;}
.album_comment_tab_copyright .type4 {background-position: -23px 0px;}
.album_comment_tab_copyright .type5 {background-position: -43px 0px;}
.album_comment_tab_copyright .type6 {background-position: -82px 0px;}
.album_comment_tab_copyright span{
	float: left;
	overflow: hidden;
	height: 18px;
}
.album_comment_tab_copyright t1 {width: 16px;}
.album_comment_tab_copyright t2 {width: 16px;}
.album_comment_tab_copyright t3 { width: 48px;}
.album_comment_tab_copyright t4 { width: 48px;}
.album_comment_tab_copyright t5 { width: 32px;}
.album_comment_tab_copyright t6 { width: 32px;}
.album_comment_tab_copyright t7 { width: 32px;}
.album_comment_tab_copyright t8 { width: 16px;}

