.content_title_tipbox {
  height: 52px;
  line-height: 52px;
  color: #eee;
  font-size: 14px;
  text-align: center;
  color: #8a8880;
}
.content_photobox_tipbox { color: #868686;}
.title_photo_delete,
.title_photo_edit {
  position: absolute;
  width: 50px;
  font-size: 12px;
  height: 14px;
  color: #b6b3ad;
  top: 11px;
  text-align: left;
  cursor: pointer;
}
.content_title_tipbox sup { font-size: 20px;}
.title_tipbox_ul { display: inline-block;}
.title_tipbox_ul li {
  padding: 0 5px;
  border-right: 1px solid #b6b3ad;
  float: left;
  height: 16px;
  line-height: 16px; 
}
.title_tipbox_ul li:last-child { border-right: none;}
/*选项卡*/
.photo-arrange {
  width: 190px;
  height: 20px;
  margin: 0 auto;
  padding-top: 30px;
}
.photo-arrange:after {
  display: block;
  clear: both;
  content: "";
}
.photo-arrange a {
  float: left;
  width: 20px;
  height: 20px;
  padding: 0 20px;
}
.photo-arrange span {
  float: left;
  color: #b6b3ad;
  line-height: 11px;
  font-size: 12px;
}
.photo-arrange i {
  display: block;
  width: 20px;
  height: 15px;
  background: url(/static/images/spirit2.svg) no-repeat;
}
.photo-arrange .photo-arrange-normal i{ background-position: -147px -150px;}
.photo-arrange .photo-arrange-free i{ background-position: -197px -150px;}
.photo-arrange .photo-arrange-full i{ background-position: -247px -150px;}

.photo-arrange .photo-normal-cur i,
.photo-arrange-normal:hover i { background-position: -147px -100px;}
.photo-arrange .photo-free-cur i,
.photo-arrange-free:hover i { background-position: -197px -100px;}
.photo-arrange .photo-full-cur i,
.photo-arrange-full:hover i { background-position: -247px -100px;}

.photo-section { 
  width: 1190px;
  margin: 0 auto;
  min-height: 330px;
}
.photo_section_normal { margin-top: 20px;}
.photo_section_normal:after {
  display: table;
  content: "";
  clear: both;
}
.photo_section_normal li {
  width: 280px;
  height: 280px;
  margin: 0 23px 20px 0;
  float: left;
  position: relative;
  cursor: pointer;
  overflow: hidden;
  background: #d8d6d0;
}
.photo_section_normal li:nth-child(4n) { margin-right: 0;}
.photo_section_normal li img {
  width: 280px;
  height: 280px;
}

.photo_section_free {
  position: relative;
  width: 1212px;
  height: auto;
  margin-top: 20px;
}
.photo_section_free li { 
  position: absolute;
  width: 280px;
  height: auto;
  cursor: pointer;
  overflow: hidden;
  -webkit-transition: all .5s ease-out .1s; 
  transition: all .5s ease-out .1s; 
}
.photo_section_free img {
  width: 280px;
  height: auto;
}
.loading {
  width: 100%;
  padding-top: 80px;
  text-align: center;
}
.loading img {
  width: 58px;
  height: 10px;
}

.photo_section_full {
  width: 860px;
  margin: 20px auto 0;
  text-align: center;
}
.photo_section_full li{
  position: relative;
  display: inline-block;
  height: auto;
  padding-bottom: 51px;
  margin: 0 auto 30px;
  text-align: center;
}
.photo_section_full img {
  max-width: 860px;
  height: auto;
  cursor: pointer;
}

/*小卡片*/
.photo_name { 
  color: #8a8880;
  padding-left: 10px;
  float: left;
}
.photo_name span {
    display: inline-block;
    font-size: 12px;
    line-height: 36px;
    color: #b6b3ad;
    -webkit-transform: scale(.75,.75);
    -ms-transform: scale(.75,.75);
    transform: scale(.75,.75);
}
.photo_section_full .photo_fixbox {
  height: 31px;
  padding-top: 19px;
  line-height: 12px;
  bottom: 0;
  border-bottom: 1px solid #dcdcdc; 
  background: #fff;
  text-align: center;
}
.photo_section_full .photo_name { 
  float: none;
  display: inline-block;
  max-width: 200px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.photo_section_full .photo_like_a {
  display: inline-block;
}
.photo_section_full .photo_fixbox button { 
  display: inline-block;
  float: none;
  margin-top: 0;
  padding: 0 20px;
}
.photo_section_full .photo_fixbox_ding { background-position: 14px -500px;}
.photo_section_full .photo_fixbox_ding:hover { background-position: 14px -600px;}
.photo_section_full .photo_fixbox_dinged { background-position: 14px -550px;}
.photo_section_full .photo_fixbox_like { background-position: -86px -500px;}
.photo_section_full .photo_fixbox_like:hover { background-position: -86px -600px;}
.photo_section_full .photo_fixbox_liked { background-position: -86px -550px;}

