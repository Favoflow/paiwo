    background: url(/static/images/spirit.svg) no-repeat -300px -350px;
    height: 40px;
    width: 40px;
    cursor: pointer;
}
.pocket-go-top:hover{
    background:url(/static/images/pocket/go_top_hover.png);
}

.social-roll-pocket{
    height: 180px;
}

.social-roll-pocket-shadow{
    z-index: 2;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(33,33,33,.3);
}

