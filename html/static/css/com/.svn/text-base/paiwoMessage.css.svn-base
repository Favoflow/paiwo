/*正在输入的样式和动画*/
.whriting:before,
.whriting:after,
.whriting {
  display: inline-block;
  border-radius: 50%;
  width: 4px;
  height: 4px;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  -webkit-animation: load 2.0s infinite ease-in-out;
  animation: load 2.0s infinite ease-in-out;
}
.whriting {
  margin: 4px auto;
  position: relative;
  text-indent: -999em;
  -webkit-animation-delay: 0.4s;
  animation-delay: 0.4s;
}
.whriting:before,
.whriting:after {
  content: '';
  position: absolute;
  top: 0;
}
.whriting:before { left: -8px;}
.whriting:after {
  left: 8px;
  -webkit-animation-delay: 0.8s;
  animation-delay: 0.8s;
}
@-webkit-keyframes load {
  0%,
  80%,
  100% { box-shadow: 0 4px 0 -1.3em #d8d6d0;}
  40% { box-shadow: 0 4px 0 0 #d8d6d0;}
}
@keyframes load {
  0%,
  80%,
  100% { box-shadow: 0 4px 0 -1.3em #d8d6d0;}
  40% { box-shadow: 0 4px 0 0 #d8d6d0;}
}

