.original-selected-box .on-addDemand-fixed,
.decorate-selected-box .decorate-selected-fixed,
.decorate-selected-box .decorate-selected-give {
  width: 60px;
  height: 65px;
  position: absolute;
  right: 0;
  top: 0;
}
.original-selected-box .on-addDemand-needfix { background: url(/static/images/cloud/cloud.svg) no-repeat 0 0;}
.original-selected-box .on-addDemand-fixding { background: url(/static/images/cloud/cloud.svg) no-repeat -200px 0;}
.original-selected-box .on-addDemand-give { background: url(/static/images/cloud/cloud.svg) no-repeat 0 -100px;}
.original-selected-box .on-addDemand-fixed { background: url(/static/images/cloud/cloud.svg) no-repeat -100px 0;}
.decorate-selected-box .decorate-selected-fixed { background: url(/static/images/cloud/cloud.svg) no-repeat -100px 0;}
.decorate-selected-box .decorate-selected-give { background: url(/static/images/cloud/cloud.svg) no-repeat 0 -100px;}
.add-dem-left-fixing,
.add-dem-left-fixed {
  position: absolute;
/*
  right: 0;
  top: 0;
*/
  top: 0;
  right: -202px;
  width: 202px;
  height: 44px;
}
.img-style{
    position: absolute;
    top: 0;
}
.add-dem-left-fixed { background: url(/static/images/cloud/cloud.svg) no-repeat 0 -250px;}
.add-dem-left-fixing { background: url(/static/images/cloud/cloud.svg) no-repeat 0 -200px;}
.on-subCustomer .title_tip { background: url(/static/images/spirit2.svg) no-repeat -700px -50px;}
.dem-big-pic-check {
  width: 90px;
  height: 30px;
  border: 1px solid #b6b3aa;
  color: #b6b3aa;
  border-radius: 15px;
  position: absolute;
  right: 153px;
  bottom: -60px;
  cursor: pointer;
}
.dem-big-pic-check:hover { border-color: #ff475c; color: #ff475c;}
.dem-big-pic-icobox {
  width: 80px;
  height: 30px;
  position: absolute;
  left: 50%;
  margin-left: -425px;
  bottom: -60px;
}
.dem-big-pic-icobox a {
  width: 31px;
  height: 31px;
  border: 1px solid #b6b3aa;
  border-radius: 50%;
}
.dem-big-pic-icobox .back { float: left; background: url(/static/images/spirit2.svg) no-repeat -750px -50px; border: none;}
.dem-big-pic-icobox .fixpic-download-icon { width: 27px; height: 27px; float:left; background: url(/static/images/spirit2.svg) no-repeat -644px -43px;}