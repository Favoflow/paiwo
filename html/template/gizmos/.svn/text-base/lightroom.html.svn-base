{% block commonjs %}
{% include "../commonjs.html" %}
{% end %}
<script>
var setM = null;
function showMessage(content){
    clearTimeout(setM);
    $('.setting_succeed').html(content).animate({top:0}, 400, function(){
            setM = setTimeout(hideMessage, 1800);
    
    });
}

function hideMessage(){
    $('.setting_succeed').animate({top:'-40px'},400);
}
</script>
